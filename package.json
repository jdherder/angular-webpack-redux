{
  "name": "angular-webpack-seed",
  "version": "1.0.0",
  "description": "An Angular 1.x seed project using webpack.",
  "main": "src/app.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=dev npm run startDevServer",
    "start:prod": "cross-env NODE_ENV=production npm run startDevServer",
    "build": "npm run build:prod",
    "build:dev": "cross-env NODE_ENV=dev npm run buildDistFiles",
    "build:prod": "cross-env NODE_ENV=production npm run buildDistFiles",
    "test": "karma start",
    "startDevServer": "webpack-dev-server --inline --progress --compress",
    "buildDistFiles": "rimraf dist && webpack"
  },
  "author": "Josh Herder",
  "license": "ISC",
  "devDependencies": {
    "angular-mocks": "^1.6.4",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^2.0.3",
    "ng-annotate-loader": "^0.6.1",
    "node-sass": "^4.5.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "angular": "^1.6.4",
    "angular-animate": "^1.6.4",
    "@uirouter/angularjs": "latest",
    "ng-redux": "^3.4.0-beta.1",
    "redux": "^3.6.0"
  }
}
